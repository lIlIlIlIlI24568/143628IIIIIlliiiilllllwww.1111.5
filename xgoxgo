MainSection:NewButton("éŸ³ä¹æ’­æ”¾å™¨  xgoç‰ˆæœ¬", "éŸ³ä¹æ’­æ”¾å™¨",function()     -- Create the GUI
local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local UICorner = Instance.new("UICorner")
local CloseButton = Instance.new("TextButton")
local TextLabel = Instance.new("TextLabel")
local BetaText = Instance.new("TextLabel")
local UICornerForClose = Instance.new("UICorner")
local TextBox = Instance.new("TextBox")
local PlayButton = Instance.new("TextButton")
local Sound = Instance.new("Sound")
-- å°†GUIåˆ°ç©å®¶çš„å±å¹•
ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ResetOnSpawn = false
-- å¸§å±æ€§ï¼ˆä¸»GUIçª—å£ï¼‰
Frame.Size = UDim2.new(0, 300, 0, 100)
Frame.Position = UDim2.new(0.5, -150, 0.5, -100)
Frame.BackgroundColor3 = Color3.fromRGB(1, 1, 1)
Frame.Parent = ScreenGui
Frame.Active = true
Frame.Draggable = true  -- ä½¿å®ƒæ‹–ç´¯
-- UIè§’ç”¨äºæ¡†æ¶ä¸Šçš„å…‰æ»‘è¾¹ç¼˜
UICorner.CornerRadius = UDim.new(0.1, 0)  -- å…‰æ»‘çš„è¾¹ç¼˜
UICorner.Parent = Frame
-- Close Button (a circle with 'X' in it)
